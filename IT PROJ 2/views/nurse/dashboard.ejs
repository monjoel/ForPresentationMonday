     */
    var data = [], totalPoints = 100;
    var donutData = [
      {
        label: "Military Dependent",
        data: <%= chart[0].military_dependent%>,
        color: "#228B22"
      },
      {
        label: "Civilian",
        data: <%= chart[0].civilian%>,
